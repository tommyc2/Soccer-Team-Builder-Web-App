<table class="ui fixed striped table">
  <thead>
    <tr>
      <th>Team</th>
      <th>Market Value</th>
      <th>Age</th>
      <th>Position</th>
    </tr>
  </thead>
  <tbody>
    {{#each players}}
      <tr>
        <td>
          {{teamName}}
        </td>
        <td>
          {{marketValue}}
        </td>
        <td>
          {{age}}
        </td>
        <td>
          {{position}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
